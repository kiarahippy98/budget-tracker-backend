<h1>New Category</h1>

<%= form_with model: @category do |f| %>
  <div>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>

  <div>
    <%= f.submit %>
  </div>
<% end %>

<%= link_to "Back", categories_path %>

<h1>Edit Transaction</h1>

<%= render "form", transaction: @transaction %>

<%= link_to "Back", transactions_path %>
